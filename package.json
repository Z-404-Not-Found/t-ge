{
  "name": "t-ge",
  "version": "0.0.5",
  "description": "t哥是一款支持与chatGPT等大语言模型聊天的桌面应用",
  "main": "./out/main/index.js",
  "author": "Sure Zhang<Zhsh_404-NotFound_P@outlook.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "chcp 65001 && electron-vite dev --watch --inspect=9229",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --publish never",
    "prepare": "husky"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "better-sqlite3": "^11.7.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.1.7",
    "openai": "^4.79.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@primevue/auto-import-resolver": "^4.2.5",
    "@primevue/themes": "^4.2.5",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.20",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "md-editor-v3": "^5.2.1",
    "moment": "^2.30.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.5",
    "sass": "^1.83.4",
    "tailwindcss": "^3.4.17",
    "tailwindcss-primeui": "^0.4.0",
    "typescript": "^5.5.2",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^5.3.1",
    "vue": "^3.4.30",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.0.22",
    "vuetify": "^3.7.6"
  }
}
